<form action="/" method="post">
  <div class="form-group">
    <label for="nome">nome</label>
    <input
      type="text"
      class="form-control"
      id="nome"
      name="nome"
      placeholder="nome"
      required
    />
  </div>
  <div class="form-group">
    <label for="id">id</label>
    <input
      type="text"
      class="form-control"
      id="id"
      name="id"
      placeholder="Enter ID"
      required
    />
  </div>
  <button type="submit" class="btn btn-primary">Adicionar Carro</button>
</form>