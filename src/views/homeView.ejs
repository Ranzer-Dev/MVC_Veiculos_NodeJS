<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Carros</h1>
    <table class="table table-striped">
        <thead>
          <tr>
            <th scope="col">Linha</th>
            <th scope="col">Id</th>
            <th scope="col">Nome</th>
          </tr>
        </thead>
        <tbody>
            <% for(var i = 0; i < vehicle.length; i++) { %>
                <tr>            
                    <th scope="row"><%= i+1 %></th>
                    <td><%= vehicle[i].id %></td>
                    <td><%= vehicle[i].nome %></td>
                    <td><a href="/delete/<%= vehicle[i].id %>" onclick="return confirm('Are you sure you want to delete this contact?');"><i class="fa-solid fa-trash"></a></i></td>               
                  </tr> 
              <% } %>
        </tbody>
      </table>
      <p><%-include('addView.ejs');%></p>
      <p><% if(saved){ %> <%-include('carroAddView.ejs');%> <% } %></p>
</body>
</html>